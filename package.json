{
  "name": "@gridiron/master",
  "version": "0.0.1",
  "description": "",
  "author": "Michael Speed",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:nest": "nest build",
    "format": "prettier --write \\\"server/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "start:nest": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/server/server/main",
    "start:nuxt": "nuxt",
    "launch": "node dist/src/main.js",
    "build-s": "run-s clean:dist compile:server compile:client copy:.nuxt copy:client copy:config",
    "build-core": "nest build core",
    "build": "tsc -p tsconfig.build.json",
    "clean:dist": "rimraf dist",
    "compile:server": "tsc -p tsconfig.build.json",
    "compile:client": "cross-env mode=production nuxt build",
    "copy:client": "copyfiles -a \"client/**/*\" dist",
    "copy:.nuxt": "copyfiles -a \".nuxt/**/*\" dist",
    "copy:config": "copyfiles nuxt.config.ts package.json package-lock.json dist",
    "dev:server": "env NODE_ENV=development nodemon src/main.ts",
    "dev:worker": "env NODE_ENV=development nodemon src/main.worker.ts",
    "run:server": "ts-node -r tsconfig-paths/register src/main.ts",
    "run:worker": "ts-node -r tsconfig-paths/register src/main.worker.ts",
    "dev": "concurrently yarn:run*",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@nestjs-query/core": "^0.17.10",
    "@nestjs-query/query-graphql": "^0.17.10",
    "@nestjs-query/query-typeorm": "^0.17.10",
    "@nestjs/common": "^7.0.13",
    "@nestjs/core": "^7.0.9",
    "@nestjs/graphql": "^7.3.9",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/microservices": "^7.0.11",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjsx/crud": "^4.6.1",
    "@nestjsx/crud-typeorm": "^4.6.1",
    "@nuxt/typescript-runtime": "^0.4.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@prisma/client": "^2.0.0",
    "apollo-client": "^2.6.10",
    "apollo-server-express": "^2.16.1",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "core-js": "^2.6.5",
    "dataloader": "^2.0.0",
    "dateformat": "^3.0.3",
    "fast-cartesian": "^5.0.0",
    "file-type": "^14.4.0",
    "fs-extra": "^9.0.1",
    "graphql": "^15.0.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^5.0.0",
    "graphql-type-json": "^0.3.1",
    "graphql-upload": "^11.0.0",
    "handlebars": "^4.7.6",
    "http-proxy-middleware": "^1.0.3",
    "image-size": "^0.8.3",
    "lodash": "^4.17.20",
    "mjml": "^4.7.0",
    "moment": "^2.26.0",
    "mysql": "^2.18.1",
    "nestjs-enlighten": "^0.0.4-beta",
    "nestjs-pino": "^1.2.0",
    "nodemailer": "^6.4.11",
    "nuxt": "^2.14.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sharp": "^0.25.3",
    "slugify": "^1.4.5",
    "typeorm": "^0.2.25",
    "uniqid": "^5.2.0"
  },
  "resolutions": {
    "fs-capacitor": "6.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nuxt/typescript-build": "^1.0.0",
    "@prisma/cli": "^2.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/lodash": "^4.14.161",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-plugin-import": "^1.13.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "fork-ts-checker-webpack-plugin": "3.1.1",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@gridiron/core/(.*)": "<rootDir>/libs/core/src/$1",
      "@gridiron/core": "<rootDir>/libs/core/src",
      "@gridiron/asset-server-plugin/(.*)": "<rootDir>/libs/asset-server-plugin/src/$1",
      "@gridiron/asset-server-plugin": "<rootDir>/libs/asset-server-plugin/src",
      "@gridiron/email-plugin/email-plugin/(.*)": "<rootDir>/libs/email-plugin/src/$1",
      "@gridiron/email-plugin/email-plugin": "<rootDir>/libs/email-plugin/src"
    }
  }
}
